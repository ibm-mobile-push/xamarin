<ContentPage 
	xmlns="http://xamarin.com/schemas/2014/forms" 
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
	x:Class="Sample.InboxPage"
	xmlns:local="clr-namespace:Sample;assembly=Sample"
	Title="Inbox">
	<ContentPage.ToolbarItems>
		<ToolbarItem Name="Sync" x:Name="SyncButton" Icon="refresh" />
	</ContentPage.ToolbarItems>
	<TableView HasUnevenRows="true" x:Name="InboxTableView">
        <TableRoot>
            <TableSection x:Name="InboxMessages" />>
        </TableRoot>
    </TableView>
</ContentPage>
