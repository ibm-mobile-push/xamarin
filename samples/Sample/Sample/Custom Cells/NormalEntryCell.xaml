<ViewCell 
	xmlns="http://xamarin.com/schemas/2014/forms" 
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
	x:Class="Sample.NormalEntryCell" 
	Height="44">
	<StackLayout Orientation="Horizontal" Padding="15,0" x:Name="Layout">
		<Label x:Name="TextLabel" HorizontalOptions="Start" VerticalOptions="CenterAndExpand" />
		<Entry x:Name="TextEntry" HorizontalOptions="EndAndExpand" VerticalOptions="CenterAndExpand" />
	</StackLayout>
</ViewCell>
